{
  "name": "gpd",
  "version": "0.12.4",
  "main": "./lib/todos",
  "private": true,
  "description": "GPD productivity system for Atom.",
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/giampierod/gpd.atom"
  },
  "license": "Apache-2.0",
  "engines": {
    "atom": ">0.50.0"
  },
  "activationCommands": {
    "atom-workspace": [
      "gpd:new-todo",
      "gpd:select-todo",
      "gpd:done-todo",
      "gpd:done-todo-and-repeat",
      "gpd:toggle-note",
      "gpd:start_timer",
      "gpd:abort_timer",
      "gpd:toggle-pomodoro"
    ]
  },
  "dependencies": {
    "underscore-plus": "1.x",
    "moment": "2.x",
    "atom-space-pen-views": "^2.0.3"
  },
  "readmeFilename": "README.md",
  "homepage": "https://github.com/giampierod/gpd.atom"
}
